{
  "name": "gorated/simple-plyr",
  "description": "A Simple Plyr Video Player Plugin",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://wordpress.org/plugins/simple-plyr/",
  "authors": [
    {
      "name": "bestony",
      "homepage": "https://github.com/bestony"
    },
    {
      "name": "jcchikikomori",
      "homepage": "https://github.com/jcchikikomori"
    }
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.6",
    "ricardofiorani/php-video-url-parser": "^1.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "object-calisthenics/phpcs-calisthenics-rules": "*",
    "phpcompatibility/php-compatibility": "*",
    "wp-coding-standards/wpcs": "*",
    "automattic/vipwpcs": "*"
  },
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ],
    "post-install-cmd": [
      "@install-codestandards"
    ]
  }
}